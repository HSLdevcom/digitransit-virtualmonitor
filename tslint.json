{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "jsx-no-lambda": false,
    "no-unused-expression": false,
    "no-unused-variable": false,
    "no-console": "error"
  }
}
